{
  "name": "waifu",
  "version": "1.0.0",
  "description": "Static web to Windows exe for local Ollama",
  "main": "main.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder --mac"
  },
  "build": {
    "appId": "com.example.waifu",
    "productName": "waifu Ollama Web",
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^31.7.7",
    "electron-builder": "^24.13.3"
  }
}
